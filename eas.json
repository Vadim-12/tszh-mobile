{
  "cli": {
    "requireCommit": true,
    "appVersionSource": "remote"
  },
  "build": {
    "beta": {
      "environment": "preview",
      "channel": "beta",
      "distribution": "internal",
      "env": {
        "APP_ENV": "beta"
      }
    },
    "production": {
      "environment": "production",
      "channel": "production",
      "autoIncrement": true,
      "env": {
        "APP_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "production"
      }
    }
  }
}
