{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"],
			"@/app/*": ["./app/*"]
		}
	},
	"include": [
		"src/**/*.ts",
		"src/**/*.tsx",
		"app/**/*.ts",
		"app/**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"nativewind-env.d.ts"
	]
}
